#include<iostream>
#include<math.h>

using namespace std;

int main(){
    const int n = 5;
    
    int x1, y1;
    
    int A[n][n] = {0};
    for(int i = 0;i<n;i++){
        for(int j = 0;j<n;j++){
            cin>>A[i][j];
            if(A[i][j] == 1){
                x1 = i;
                y1 = j;
                i = n+1;
            }
        }
        
    }
    cout<<abs(x1-2) + abs(y1-2);
    return 0;
    
    
}
